@{
}

        <nav aria-label="Page navigation example">
        <ul class="pagination mt-2">
        @for (int i = 1; i <=ViewBag.TotalPages; i++)
        {

                <li class="page-item @(i==ViewBag.currentstandingPage ? "active": " ")"><a class="page-link" asp-action="Index" 
                asp-route-currentPage="@i" asp-route-catId="@ViewBag.Category" >@i</a>
                </li>       
                   
        }
        </ul>
        </nav>